<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app1.will.itsyourcall" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".GameScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CategoryScreen"
            android:label="@string/title_activity_category_screen" >
        </activity>
        <activity
            android:name=".QuestionScreen"
            android:label="@string/title_activity_question_screen" >
        </activity>
        <activity
            android:name=".ResultsScreen"
            android:label="@string/title_activity_results_screen" >
        </activity>
    </application>

</manifest>
